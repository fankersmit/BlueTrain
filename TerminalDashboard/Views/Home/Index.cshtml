@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>

    <ul>
        @for ( int i = 0 ; i < (int)ViewData["TerminalCount"]; i++ )
        {
            var key = string.Format("RPI0{0}", i + 1);
            <li> @key : @ViewData[key] </li>
        }
    </ul>

</div>
